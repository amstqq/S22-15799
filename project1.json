{
    "database_system": "postgres",
    "benchmark_name": "epinions",
    "number_of_actual_runs": 1,
    "algorithms": [
        {
            "name": "drop",
            "parameters": {
                "max_indexes": [4, 6, 8, 10, 12, 14]
            },
            "timeout": 120
        },
        {
            "name": "auto_admin",
            "parameters": {
                "max_indexes": [4, 6, 8, 10, 12, 14],
                "max_indexes_naive": 2,
                "max_index_width": 2
            },
            "timeout": 300
        },
        {
            "name": "extend",
            "parameters": {
                "budget_MB": [3000, 5000, 10000, 15000, 30000],
                "max_index_width": 2
            },
            "timeout": 120
        },
        {
            "name": "relaxation",
            "parameters": {
                "budget_MB": [3000, 5000, 10000, 15000, 30000],
                "max_index_width": 2
            },
            "timeout": 120
        },
        {
            "name": "anytime",
            "parameters": {
                "max_index_width": 2,
                "budget_MB":  [3000, 5000, 10000, 15000, 30000],
                "max_runtime_minutes": 60
            },
            "timeout": 120
        },
        {
            "name": "db2advis",
            "parameters": {
                "max_index_width": 2,
                "budget_MB":  [3000, 5000, 10000, 15000, 30000],
                "try_variations_seconds": 5
            },
            "timeout": 120
        }
    ]
}
